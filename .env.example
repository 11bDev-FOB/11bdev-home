# Rails Configuration
# Rails Master Key (keep secret, do not commit actual value)
# Get from: config/master.key
RAILS_MASTER_KEY=

# Admin Authentication
# Used for HTTP Basic Auth on /admin routes
ADMIN_USERNAME=admin
ADMIN_PASSWORD=changeme

# GitHub API Token (optional, for higher rate limits)
# Create at: https://github.com/settings/tokens
# Recommended scopes: read:org (public org reads)
GITHUB_TOKEN=

# SMTP settings (only if you enable ActionMailer in production)
SMTP_ADDRESS=
SMTP_PORT=587
SMTP_DOMAIN=11b.dev
SMTP_USERNAME=
SMTP_PASSWORD=
SMTP_AUTH=plain
